generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id String @id @default(uuid())

  name        String
  description String
  image_name  String
  value       Int
  quantity    Int

  created_at DateTime @default(now())

  @@map("products")
}
